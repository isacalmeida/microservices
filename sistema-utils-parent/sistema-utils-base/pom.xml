<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>br.edu.unoesc.sistemautils</groupId>
		<artifactId>sistema-utils-parent</artifactId>
		<version>1.0.0</version>
	</parent>

	<groupId>br.edu.unoesc.sistemautils.base</groupId>
	<artifactId>sistema-utils-base</artifactId>
	<packaging>pom</packaging>

	<properties>
		<acesso-service.version>1.0.0</acesso-service.version>
		<admin-server.version>1.0.0</admin-server.version>
		<auth-server.version>1.0.0</auth-server.version>
		<config-server.version>1.0.0</config-server.version>
		<consul-server.version>1.0.0</consul-server.version>
		<eureka-server.version>1.0.0</eureka-server.version>
		<frontend-server.version>1.0.0</frontend-server.version>
		<hystrix-server.version>1.0.0</hystrix-server.version>
		<operacao-service.version>1.0.0</operacao-service.version>
		<pessoa-service.version>1.0.0</pessoa-service.version>
		<transicao-service.version>1.0.0</transicao-service.version>
		<ui-service.version>1.0.0</ui-service.version>
		<zipkin-server.version>1.0.0</zipkin-server.version>
		<zuul-server.version>1.0.0</zuul-server.version>
		
		<node.version>v12.11.1</node.version>
		<npm.version>6.12.1</npm.version>
		
		<webjars-locator.version>0.37</webjars-locator.version>
		<webjars-jquery.version>3.4.1</webjars-jquery.version>
		<webjars-bootstrap.version>3.4.1</webjars-bootstrap.version>
		<webjars-fa.version>5.10.1</webjars-fa.version>
		<webjars-select2.version>4.0.8</webjars-select2.version>
		<webjars-datepicker.version>1.9.0</webjars-datepicker.version>
		<webjars-mask.version>1.14.15</webjars-mask.version>
		<webjars-adminlte.version>2.4.15</webjars-adminlte.version>
		<webjars-switch.version>3.3.4</webjars-switch.version>
		<webjars-jqueryui.version>1.12.1</webjars-jqueryui.version>
		<webjars-autocomplete.version>1.4.10</webjars-autocomplete.version>
		<webjars-icheck.version>1.0.2</webjars-icheck.version>

		<compiler-args.properties>--enable-preview</compiler-args.properties>
		<jar-index.properties>true</jar-index.properties>

		<docker-repository.properties>isacalmeida</docker-repository.properties>
		<docker-info.properties>true</docker-info.properties>

		<graal-nativeimage.version>20.0.0</graal-nativeimage.version>
		<graal-buildargs.properties>
			--verbose 
			--no-server 
			--no-fallback 
			-Dspring.graal.verbose=true 
			--allow-incomplete-classpath 
			-H:+TraceClassInitialization 
			-H:+ReportExceptionStackTraces 
			-Dspring.graal.remove-unused-autoconfig=true 
			-Dspring.graal.missing-selector-hints=warning 
		</graal-buildargs.properties>

		<acesso-service.main-class>br.edu.unoesc.acessoservice.application.AcessoServiceApplication</acesso-service.main-class>
		<admin-server.main-class>br.edu.unoesc.adminserver.application.AdminServerApplication</admin-server.main-class>
		<auth-server.main-class>br.edu.unoesc.authserver.application.AuthServerApplication</auth-server.main-class>
		<config-server.main-class>br.edu.unoesc.configserver.application.ConfigServerApplication</config-server.main-class>
		<consul-server.main-class>br.edu.unoesc.consulserver.application.ConsulServerApplication</consul-server.main-class>
		<eureka-server.main-class>br.edu.unoesc.eurekaserver.application.EurekaServerApplication</eureka-server.main-class>
		<frontend-server.main-class>br.edu.unoesc.frontendserver.application.FrontendServerApplication</frontend-server.main-class>
		<hystrix-server.main-class>br.edu.unoesc.hystrixserver.application.HystrixServerApplication</hystrix-server.main-class>
		<operacao-service.main-class>br.edu.unoesc.operacaoservice.application.OperacaoServiceApplication</operacao-service.main-class>
		<pessoa-service.main-class>br.edu.unoesc.pessoaservice.application.PessoaServiceApplication</pessoa-service.main-class>
		<transicao-service.main-class>br.edu.unoesc.transicaoservice.application.TransicaoServiceApplication</transicao-service.main-class>
		<zipkin-server.main-class>br.edu.unoesc.zipkinserver.application.ZipkinServerApplication</zipkin-server.main-class>
		<zuul-server.main-class>br.edu.unoesc.zuulserver.application.ZuulServerApplication</zuul-server.main-class>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>br.edu.unoesc.acessoservice</groupId>
				<artifactId>acesso-service-parent</artifactId>
				<version>${acesso-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.adminserver</groupId>
				<artifactId>admin-server-parent</artifactId>
				<version>${admin-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.authserver</groupId>
				<artifactId>auth-server-parent</artifactId>
				<version>${auth-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.configserver</groupId>
				<artifactId>config-server-parent</artifactId>
				<version>${config-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.consulserver</groupId>
				<artifactId>consul-server-parent</artifactId>
				<version>${consul-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.eurekaserver</groupId>
				<artifactId>eureka-server-parent</artifactId>
				<version>${eureka-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.frontendserver</groupId>
				<artifactId>frontend-server-parent</artifactId>
				<version>${frontend-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.hystrixserver</groupId>
				<artifactId>hystrix-server-parent</artifactId>
				<version>${hystrix-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.operacaoservice</groupId>
				<artifactId>operacao-service-parent</artifactId>
				<version>${operacao-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.pessoaservice</groupId>
				<artifactId>pessoa-service-parent</artifactId>
				<version>${pessoa-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.transicaoservice</groupId>
				<artifactId>transicao-service-parent</artifactId>
				<version>${transicao-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.uiservice</groupId>
				<artifactId>ui-service-parent</artifactId>
				<version>${ui-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.zipkinserver</groupId>
				<artifactId>zipkin-server-parent</artifactId>
				<version>${zipkin-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>br.edu.unoesc.zuulserver</groupId>
				<artifactId>zuul-server-parent</artifactId>
				<version>${zuul-server.version}</version>
				<type>pom</type>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>
