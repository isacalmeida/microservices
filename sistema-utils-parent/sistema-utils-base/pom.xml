<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<artifactId>sistema-utils-parent</artifactId>
		<groupId>br.edu.unoesc.sistemautils</groupId>
		<version>1.0.0</version>
	</parent>

	<artifactId>sistema-utils-base</artifactId>
	<groupId>br.edu.unoesc.sistemautils.base</groupId>
	<packaging>pom</packaging>

	<properties>
		<!-- services properties -->
		<acesso-service.version>1.0.0</acesso-service.version>
		<admin-server.version>1.0.0</admin-server.version>
		<auth-server.version>1.0.0</auth-server.version>
		<config-server.version>1.0.0</config-server.version>
		<eureka-server.version>1.0.0</eureka-server.version>
		<hystrix-server.version>1.0.0</hystrix-server.version>
		<pessoa-service.version>1.0.0</pessoa-service.version>
		<ui-service.version>1.0.0</ui-service.version>
		<zipkin-server.version>1.0.0</zipkin-server.version>
		<zuul-server.version>1.0.0</zuul-server.version>
		<!-- services properties -->

		<!-- back-end properties -->
		<activation-api.version>1.2.0</activation-api.version>
		<jaxb-api.version>2.3.1</jaxb-api.version>
		<jaxb-runtime.version>2.3.2</jaxb-runtime.version>
		<reflections.version>0.9.12</reflections.version>
		<!-- back-end properties -->

		<!-- front-end properties -->
		<webjars-locator.version>0.37</webjars-locator.version>
		<webjars-jquery.version>3.4.1</webjars-jquery.version>
		<webjars-bootstrap.version>3.4.1</webjars-bootstrap.version>
		<webjars-fa.version>5.10.1</webjars-fa.version>
		<webjars-select2.version>4.0.8</webjars-select2.version>
		<webjars-datepicker.version>1.9.0</webjars-datepicker.version>
		<webjars-mask.version>1.14.15</webjars-mask.version>
		<webjars-adminlte.version>2.4.15</webjars-adminlte.version>
		<webjars-switch.version>3.3.4</webjars-switch.version>
		<webjars-jqueryui.version>1.12.1</webjars-jqueryui.version>
		<webjars-autocomplete.version>1.4.10</webjars-autocomplete.version>
		<webjars-icheck.version>1.0.2</webjars-icheck.version>
		<!-- front-end properties -->
		
		<!-- maven properties -->
		<compiler-args.properties>--enable-preview</compiler-args.properties>
		<jar-index.properties>true</jar-index.properties>
		<!-- maven properties -->
		
		<!-- docker properties -->
		<dockerfile-spotify.version>1.4.13</dockerfile-spotify.version>
		<docker-repository.properties>isacalmeida</docker-repository.properties>
		<docker-info.properties>true</docker-info.properties>
		<!-- docker properties -->
		
		<!-- graal properties -->
		<graal-nativeimage.version>20.0.0</graal-nativeimage.version>
		<graal-buildargs.properties>
			--verbose
			--no-server
			--no-fallback 
			-Dspring.graal.verbose=true
			--allow-incomplete-classpath
			-H:+ReportExceptionStackTraces 
			-Dspring.graal.remove-unused-autoconfig=true 
		</graal-buildargs.properties>
		<!-- graal properties -->
		
		<!-- main classes properties -->
		<acesso-service.main-class>br.edu.unoesc.acessoservice.application.boot.AcessoServiceApplication</acesso-service.main-class>
		<admin-server.main-class>br.edu.unoesc.adminserver.application.boot.AdminServerApplication</admin-server.main-class>
		<auth-server.main-class>br.edu.unoesc.authserver.application.boot.AuthServerApplication</auth-server.main-class>
		<config-server.main-class>br.edu.unoesc.configserver.application.boot.ConfigServerApplication</config-server.main-class>
		<eureka-server.main-class>br.edu.unoesc.eurekaserver.application.boot.EurekaServerApplication</eureka-server.main-class>
		<hystrix-server.main-class>br.edu.unoesc.hystrixserver.application.boot.HystrixServerApplication</hystrix-server.main-class>
		<pessoa-service.main-class>br.edu.unoesc.pessoaservice.application.boot.PessoaServiceApplication</pessoa-service.main-class>
		<zipkin-server.main-class>br.edu.unoesc.zipkinserver.application.boot.ZipkinServerApplication</zipkin-server.main-class>
		<zuul-server.main-class>br.edu.unoesc.zuulserver.application.boot.ZuulServerApplication</zuul-server.main-class>
		<!-- main classes properties -->
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<artifactId>acesso-service-parent</artifactId>
				<groupId>br.edu.unoesc.acessoservice</groupId>
				<version>${acesso-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>admin-server-parent</artifactId>
				<groupId>br.edu.unoesc.adminserver</groupId>
				<version>${admin-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>auth-server-parent</artifactId>
				<groupId>br.edu.unoesc.authserver</groupId>
				<version>${auth-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>config-server-parent</artifactId>
				<groupId>br.edu.unoesc.configserver</groupId>
				<version>${config-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>eureka-server-parent</artifactId>
				<groupId>br.edu.unoesc.eurekaserver</groupId>
				<version>${eureka-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>hystrix-server-parent</artifactId>
				<groupId>br.edu.unoesc.hystrixserver</groupId>
				<version>${hystrix-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>pessoa-service-parent</artifactId>
				<groupId>br.edu.unoesc.pessoaservice</groupId>
				<version>${pessoa-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>ui-service-parent</artifactId>
				<groupId>br.edu.unoesc.uiservice</groupId>
				<version>${ui-service.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>zipkin-server-parent</artifactId>
				<groupId>br.edu.unoesc.zipkinserver</groupId>
				<version>${zipkin-server.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<artifactId>zuul-server-parent</artifactId>
				<groupId>br.edu.unoesc.zuulserver</groupId>
				<version>${zuul-server.version}</version>
				<type>pom</type>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>